/*Global*/

html  { {% if settings.use_bg %}
background: url("{{ '' | asset_url }}") repeat left top transparent !important;
{% endif %}
background-color: {{ settings.html_bg_color }}

}


body  { {% case settings.body_bg_type %}
{% when 'bg_image' %}
background: url("{{ '' | asset_url }}") repeat left top transparent !important;
{% when 'bg_color' %}
background-color: {{ settings.body_bg_color }}
{% endcase %}
}


body{font-family: {{ settings.regular_font_family}}; font-size: {{ settings.regular_font_size}}; line-height: {{ settings.regular_line_height}}; color: {{ settings.font_color }};}


{% assign heading_font =  %}
{% if settings.heading_family_use_custom %}
{% assign heading_font = settings.heading_custom_font_family %}
{% else %}
{% assign heading_font = settings.heading_font_family %}
{% endif %}

a {color: {{ settings.link_color }};}
a:hover{color: {{ settings.link_color_act }};}


label{font-size: {{ settings.regular_font_size}}; line-height: {{ settings.regular_line_height}};}

input[type="text"],
input[type="email"],
input[type="password"],
input[type="telephone"]{font-size: {{ settings.regular_font_size}};line-height: {{ settings.regular_line_height}};}

select { font-size: {{ settings.regular_font_size}}; line-height: {{ settings.regular_line_height}};}

h1, h2, h3, h4, h5, h6 {margin: 0;font-family: {{ heading_font }};color: {{ settings.headings_color }};}

h1 {font-size: {{ settings.h1_fsize }}; line-height: {{ settings.h1_lheight }};}
h2 {font-size: {{ settings.h2_fsize }}; line-height: {{ settings.h2_lheight }};}
h3 {font-size: {{ settings.h3_fsize}}; line-height: {{ settings.h3_lheight }};}
h4 {font-size: {{ settings.h4_fsize }}; line-height: {{ settings.h4_lheight }};}
h5 {font-size: {{ settings.h5_fsize }};line-height: {{ settings.h5_lheight }};}
h6 {font-size: {{ settings.h6_fsize }};  line-height: {{ settings.h6_lheight }};}

.btn.btn-info,
.btn.btn-primary {background: {{ settings.btn_primary_color }}; text-transform: uppercase; border:1px {{ settings.btn_border_color }} solid; color:{{settings.btn_txt_color}} }
.btn.btn-info:hover,
.btn.btn-primary:hover {background: {{ settings.btn_primary_color_hover }};color:{{settings.btn_txt_color_hover}} !important; border:1px {{ settings.btn_border_color_hover }} solid}
.btn.btn-info a,
.btn.btn-primary a {color:{{settings.btn_txt_color}} !important;}
.btn.btn-info a:hover,
.btn.btn-primary a:hover {color:{{settings.btn_txt_color_hover}} !important;}
.btn-active {background: {{ settings.btn_act_color }}!important;border:1px {{ settings.btn_act_border_color }} solid!important;color:{{settings.btn_txt_color_hover}} !important;}

.continue_shop {background: {{ settings.cont_shop_bg }}!important; color:{{settings.cont_shop_txt}}!important; border:1px {{ settings.cont_shop_border }} solid!important}
.continue_shop:hover {background: {{ settings.check_btn_bg }}!important; color:{{settings.check_btn_txt}}!important; border:1px {{ settings.cont_shop_border }} solid!important}
.update-btn {background: {{ settings.update_cart_bg }}!important; color:{{settings.update_cart_txt}}!important; border:1px {{ settings.update_cart_border }} solid!important}
.update-btn:hover {background: {{ settings.check_btn_bg }}!important; color:{{settings.check_btn_txt}}!important; border:1px {{ settings.update_cart_border }} solid!important}
.check-btn {background: {{ settings.check_btn_bg }}!important; color:{{settings.check_btn_txt}}!important; border:1px {{ settings.check_btn_border }} solid!important}
.check-btn:hover {background: {{ settings.check_btn_bg }}!important; color:{{settings.check_btn_txt}}!important; border:1px {{ settings.check_btn_border }} solid!important}


