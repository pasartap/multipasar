/* Responsive CSS */

@media only screen and (min-width: 320px) and (max-width: 479px) {
    #main_content .product.col-sm-4 {
        width: 48% !important;
    }

    .owl-item .item {
        margin: 10px 0 5px !important;
    }

    .bestsell-pro .block-title {
        padding-right: 60px;
    }

    .bestsell-pro .block-title h2 {
        font-size: 18px;
        padding-bottom: 6px;
    }

    .bestsell-pro .slider-items-products .owl-buttons .owl-next {
        right: -45px;
        top: -8px;
    }

    .bestsell-pro .slider-items-products .owl-buttons .owl-prev {
        left: 12px;
        top: -8px;
    }

    .featured-pro .slider-items-products .owl-buttons .owl-next {
        right: 20px;
    }

    .featured-pro .slider-items-products .owl-buttons .owl-prev {
        left: -45px;
    }

    .featured-block .item-inner {
        margin: 0 5px;
    }

    #main_content .grid-uniform .product {
        margin: 0;
        width: 48%;
    }

    .brand-header {
        font-size: 18px;
    }

    .footer-newsletter #mc_embed_signup1 .button {
        margin: 10px 0;
    }

    .footer-newsletter #mc_embed_signup1 .button:hover {
        padding: 0;
    }

    .footer-newsletter #mc_embed_signup1 input.email {
        width: 100%;
    }

    #mc-embedded-subscribe1 {
        margin: 10px 0 0 0 !important;
    }

    #line-item-details,
    .item-in-cart {
        font-size: 18px;
    }

    .line-item-details .fa-shopping-cart {
        margin-right: 10px !important;
        margin-top: 0 !important;
        padding: 1px !important;
    }

    .product_wrap #product_description {
        margin-top: 10px;
    }  

    #searchresults h1.page_title {
        font-size: 16px !important;
    }

    .section-header-left {
        margin-bottom: 8px;
        width: auto;
    }

    .payment_status,
    .fulfillment_status {
        display: none;
    }

    .order_status {
        display: table-cell;
    }

    .ptwholesale-steps-container-fluid::after,
    .ptwholesale-steps-row::after {
        clear: both !important;
    }
}

@media only screen and (max-width: 600px)  {
    .product-item .col-xs-6 {
        width: 100% !important;
    }

    #searchresults h1.page_title {
        font-size: 16px !important;
    }

    .ptwholesale-steps-container-fluid::after,
    .ptwholesale-steps-row::after {
        clear: both !important;
    }
}

@media only screen and (min-width: 480px) and (max-width: 767px) {
    #main_content .product.col-sm-4 {
        width: 48%;
    }

    #main_content .grid-uniform .product {
        margin: 0;
        width: 31%;
    }

    #mc-embedded-subscribe1 {
        margin: 0 0 0 10px !important;
    }

    #line-item-details,
    .item-in-cart {
        font-size: 18px;
    }

    .line-item-details .fa-shopping-cart {
        margin-right: 10px !important;
        margin-top: 0 !important;
        padding: 1px !important;
    }

    .col-mid, .col-right {
        width: 50%;
    }

    .section-header .form-horizontal label {
        display: inline-block;
    }

    .payment_status,
    .fulfillment_status {
        display: table-cell;
    }

    .order_status {
        display: none;
    }

    .ptwholesale-steps-container-fluid::after,
    .ptwholesale-steps-row::after {
        clear: both !important;
    }
}

@media (min-width: 768px) and (max-width: 979px) {
    #main_content .product.col-sm-4 {
        width: 31%;
    }

    #mc-embedded-subscribe1 {
        margin: 10px 0 0 0 !important;
    }

    .bottom_block_5 {
        width: 33.33333333%;
    }

    #footer .newsletter-wrap {
        margin: 0;
        width: 100%;
    }

    #footer .social-links {
        float: left;
        width: 100%;
    }

    .product_wrap #product_description {
        margin-top: 10px;
    }

    .col-mid .img-block {
       margin-bottom: 20px;
    }

    .col-mid, .col-right {
        margin-top: 0;
    }

    .collection-view {
        margin-left: 0;
        float: right;
    }

    .section-header .form-horizontal label {
        display: inline-block;
    }
}

