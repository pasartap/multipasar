/*
* PasarTap CSS customization
*/

#header {
    background-image: none !important;
}

.site-nav::before,
.top-nav::before,
.site-nav::after {
    display: none !important;
}

.site-nav-dropdown {
    overflow-y: auto;
    max-height: 600px;
}

#sticky-mmenu {
    position: fixed;
    width: 100%;
    top: 0;
}

nav#sticky-menu:not( .mm-menu),
nav#sticky-cart:not( .mm-menu) {
    display: none;
}

.mh-head .mh-btns-right {
